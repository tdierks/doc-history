{
  "manifest_version": 2,
  
  "name": "Docs history",
  "description": "Tracks your most-recently used Google Docs documents for quick access",
  "author": "Tim Dierks <tim@dierks.org>",
  "version": "1.1",
  "homepage_url": "https://github.com/tdierks/doc-history",
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },

  "background": {
    "page": "doc-history.html",
    "persistent": false
  },
  
  "permissions": [
    "tabs",
    "storage"
  ],

  "omnibox": {
    "keyword": "dh"
  }
}
